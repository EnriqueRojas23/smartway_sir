@model  Web.Smartway.Areas.Recepcion.Models.DocumentoRecepcionModel
@{

}
<form id="frmDocumentacion" role="form" method="post" action="@Url.Action("SubirArchivo", "Recepcion")" class="form-horizontal" enctype="multipart/form-data">
    @*@using (Ajax.BeginForm("SubirArchivo", "Liquidacion", new { area = "Liquidacion" }, new AjaxOptions { HttpMethod = "POST", OnComplete = "OnCompleteTransaction(xhr, status)" }, new { enctype = "multipart/form-data", @role = "form", @id = "frmDocumentacion", @class = "form-horizontal" }))*@

    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
        @*<i class="fa fa-users fa-4x"></i>*@
        <h4 class="modal-title">Cargar Excel</h4>
        <small class="font-bold">Digitar los datos necesarios para el registro masivo</small>
    </div>
    <div class="modal-body">
        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <label class="col-md-2 control-label">Archivo:</label>
                    <div class="col-md-10">@Html.TextBoxFor(model => model.archivo, "", new { @class = "form-control", @type = "file" })</div>
                </div>
                <div class="form-group">
                    <input value="La extensión no está permitida" class="col-md-12 alert alert-danger" type="text" id="error" />
                </div>
            </div>

        </div>
    </div>
    <div class="modal-footer">
        @*@Html.HiddenFor(x => x.idorden)*@
        <button type="submit" class="btn  btn-line btn-primary"><i class="fa fa-upload"></i> Subir Archivo </button>
        <button type="button" class="btn  btn-outline btn-danger btn-sm" data-dismiss="modal"><i class="fa fa-times-circle-o"></i>  Cerrar</button>
    </div>
</form>
