@model Web.Smartway.Areas.Agendamiento.Models.InfoOrdenModel

<div class="ibox float-e-margins">
    <div class="ibox-title">
        <h5><i class="fa fa-info-circle"></i>  Información </h5>
        <div class="ibox-tools">
            <a class="collapse-link">
                <i class="fa fa-chevron-up"></i>
            </a>
        </div>
    </div>
    <div class="ibox-content">

        <section class="widget widget-card bg-gray">
            <div class="widget-body clearfix">
                <div class="live-tile" data-mode="carousel" data-speed="750" data-delay="4000" data-height="100">
                    <div class="bg-gray text-white">
                        <div class="row">
                            <div class="col-xs-3">
                                <span class="widget-icon">
                                    <i class="glyphicon glyphicon-briefcase"></i>
                                </span>
                            </div>
                            <div class="col-xs-9">
                                <h6 class="no-margin">Orden de Servicio</h6>
                                <p class="h2 no-margin fw-normal">OST- @Model.numeroincidencia </p>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-3">
                                <h6 class="no-margin">Cod.</h6>
                                <p class="value6">@Model.codigoproducto</p>
                            </div>
                            <div class="col-xs-9">
                                <h6 class="no-margin">Producto</h6>
                                <p class="value6">@Model.producto</p>
                            </div>
                        </div>




                    </div>
                    <div class="text-white">
                        <div class="row">
                            <div class="col-xs-3">
                                <span class="widget-icon">
                                    <i class="glyphicon glyphicon-certificate"></i>
                                </span>
                            </div>
                            <div class="col-xs-9">
                                <h6 class="no-margin">Fecha asignación</h6>
                                <p class="h2 no-margin fw-normal">@Model.fechaasignacion </p>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-12">
                                <h6 class="no-margin">Falla</h6>
                                <p class="value5">@Model.falla</p>
                            </div>
                           
                        </div>  
                    </div>
                </div>
            </div>

            <div class="panel panel-default">
                <div class="panel-body">
                    <div class="col-md-12 text-center">
                        <div class="m-b-sm">
                            <a id="lknAntecedentes" data-url="@Url.Action("Antecedentes", "Incidencia")" href="javascript:Antecedentes(@Model.codigoproducto);" class="btn  btn-block btn-primary"><strong> Ver Antecedentes </strong><i class="fa fa-long-arrow-right"></i> </a>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
</div>