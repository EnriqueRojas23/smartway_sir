@model  QueryContracts.Coolbox.Seguridad.Results.TraerControlDto

@{
    ViewBag.Title = "EditarControl";
}

<h2>Editar Control</h2>
<br />
<br />
<table>
    <tr>
        <td>Descripción: </td>
        <td>@Html.TextBoxFor(model => model.con_str_descrip)</td>
    </tr>
    <tr>
        <td>Comentario: </td>
        <td>@Html.TextBoxFor(model => model.con_str_comentario)</td>
    </tr>
</table>
<br />
<fieldset>
    <button id='newProduct1' onclick="location.href='@Url.Action("Actualizar", "Seguridad")'" class="btn btn-primary">Actualizar</button>
    <button id='newProduct2' onclick="location.href='@Url.Action("Control", "Seguridad")'" class="btn btn-primary">Cancelar</button>
</fieldset>